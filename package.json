{
  "name": "element-ui-x",
  "version": "1.0.0",
  "description": "基于Vue 2 + Element UI的企业级AI聊天组件库",
  "private": true,
  "scripts": {
    "format": "node format-project.js",
    "lint": "eslint --ext .js,.vue .",
    "lint:fix": "eslint --ext .js,.vue . --fix",
    "install:all": "npm install && cd packages/element-x-ui && npm install && cd ../../examples && npm install && cd ../docs && npm install",
    "dev:docs": "cd docs && npm run dev",
    "build:docs": "cd docs && npm run build",
    "dev:examples": "cd examples && npm run dev",
    "build:examples": "cd examples && npm run build",
    "dev": "npm run dev:examples",
    "build:lib": "cd packages/element-x-ui && npm run build",
    "publish:lib": "cd packages/element-x-ui && npm publish",
    "release": "npm run lint && npm run build:lib && npm run publish:lib"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.20.0",
    "prettier": "^2.5.1"
  }
}
